<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/settings">

    <fragment
        android:id="@+id/settings"
        android:name="dev.kdrag0n.android12ext.ui.settings.SettingsFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_settings">
        <action android:id="@+id/action_settings_to_about"
            app:destination="@id/about_libraries" />
    </fragment>

    <fragment
        android:id="@+id/about_libraries"
        android:label="@string/about"
        android:name="dev.kdrag0n.android12ext.ui.about.AboutFragment">
        <!-- From @navigation/aboutlibs_navigation -->
        <argument
            android:name="data"
            app:argType="java.io.Serializable"
            app:nullable="true" />
    </fragment>

</navigation>